<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="maskgen" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="bin\debug\maskgen" prefix_auto="1" extension_auto="1" />
				<Option object_output="bin\obj\debug\" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option parameters="mask.tga mask.mask -p sdl" />
				<Compiler>
					<Add option="-g" />
					<Add directory="$(#sdl.include)\SDL" />
				</Compiler>
				<Linker>
					<Add library="libSDL.dll.a" />
					<Add library="SDL_image" />
				</Linker>
			</Target>
			<Target title="Release">
				<Option output="bin\release\maskgen" prefix_auto="1" extension_auto="1" />
				<Option object_output="bin\obj\release\" />
				<Option type="0" />
				<Option compiler="gcc" />
				<Option parameters="Font_25_18_35.tga 640 480" />
				<Compiler>
					<Add option="-O2" />
					<Add directory="$(#sdl.include)\SDL" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add library="libSDL.dll.a" />
					<Add library="SDL_image" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add directory="$(#sdl.include)" />
		</Compiler>
		<Linker>
			<Add library="mingw32" />
			<Add library="SDLmain" />
			<Add library="SDL.dll" />
			<Add library="user32" />
			<Add library="gdi32" />
			<Add library="winmm" />
			<Add library="dxguid" />
			<Add directory="$(#sdl.lib)" />
		</Linker>
		<Unit filename="defines.h" />
		<Unit filename="main.c">
			<Option compilerVar="CC" />
		</Unit>
		<Extensions>
			<code_completion />
			<debugger />
			<envvars />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
